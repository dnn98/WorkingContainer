<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workingcontainer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="activity_full"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="activity_text"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="activity_button"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="activity_image"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="activity_checkbox"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="activity_radiobutton"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>